<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="hospital" class="com.milton.spring.springcore1.list.Hospital">
        <property name="name" value="Global Hospital"></property>
        <property name="departaments">
            <list>
                <value>Front Office</value>
                <value>In Patient</value>
                <value>Out Patient</value>
            </list>
        </property>

    </bean>
</beans>